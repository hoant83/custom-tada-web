  "scripts": {
    "start": "craco start",
    "starttruck": "yarn && REACT_APP_THEME=truckowner PORT=3003 craco start",
    "starttada": "yarn && REACT_APP_THEME=tadatruck PORT=3004 craco start",
    "startadmin": "yarn && REACT_APP_THEME=admin PORT=3005 craco start",
    "starts": "HTTPS=true craco start",
    "build": "craco build",
    "buildtruck": "REACT_APP_THEME=truckowner env-cmd -f .env.prod --no-override craco build",
    "buildtada": "REACT_APP_THEME=tadatruck env-cmd -f .env.prod --no-override craco build",
    "buildadmin": "REACT_APP_THEME=admin env-cmd -f .env.prod --no-override craco build",
    "build-st-sa-truckowner": "REACT_APP_THEME=truckowner env-cmd -f .env.staging.sa --no-override craco build",
    "build-st-sa-customer": "REACT_APP_THEME=tadatruck env-cmd -f .env.staging.sa --no-override craco build",
    "build-st-sa-admin": "REACT_APP_THEME=admin env-cmd -f .env.staging.sa --no-override  craco build",
    "build-prod-sa-truckowner": "REACT_APP_THEME=truckowner env-cmd -f .env.prod --no-override craco build",
    "build-prod-sa-customer": "REACT_APP_THEME=tadatruck env-cmd -f .env.prod --no-override craco build",
    "build-prod-sa-admin": "REACT_APP_THEME=admin env-cmd -f .env.prod --no-override  craco build",
    "test": "craco test",
    "eject": "craco eject"
  },
